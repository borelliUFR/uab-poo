package objeto.o05.polimorfismo.exercicio;

public class Arvore {
    
    private double altura;
    private String especie;
    private String nomeFruto = "desconhecido";
    
    public double getAltura() {
        return altura;
    }
    
    public void setAltura(double altura) {
        this.altura = altura;
    }

    /**
     * @return the especie
     */
    public String getEspecie() {
        return especie;
    }

    /**
     * @param especie the especie to set
     */
    public void setEspecie(String especie) {
        this.especie = especie;
    }

    /**
     * @return the nomeFruto
     */
    public String getNomeFruto() {
        return nomeFruto;
    }

    /**
     * @param nomeFruto the nomeFruto to set
     */
    public void setNomeFruto(String nomeFruto) {
        this.nomeFruto = nomeFruto;
    }
    
    
}
